            <column name="megrendeles_vege" type="date">
                <constraints nullable="true" />
            </column>
            <column name="resztvevo_kollaga_tipusa" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="devizanem" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="dijazas_tipusa" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="dij_osszege" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="megrendeles_dokumentum_generalta" type="boolean">
