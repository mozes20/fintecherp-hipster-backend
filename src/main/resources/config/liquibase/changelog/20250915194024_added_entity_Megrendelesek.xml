            <column name="feladat_rovid_leirasa" type="string"/>
            <column name="feladat_reszletes_leirasa" type="string"/>
            <column name="megrendeles_kezdete" type="date"/>
            <column name="megrendeles_vege" type="date"/>
            <column name="resztvevo_kollaga_tipusa" type="string"/>
            <column name="devizanem" type="string"/>
            <column name="dijazas_tipusa" type="string"/>
            <column name="dij_osszege" type="numeric"/>
            <column name="megrendeles_dokumentum_generalta" type="boolean"/>
            <column name="ugyfel_megrendeles_id" type="numeric"/>
            <!-- jhipster-needle-liquibase-add-loadcolumn - JHipster (and/or extensions) can add load columns here -->
