            <column name="feladat_rovid_leirasa" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="feladat_reszletes_leirasa" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="megrendeles_kezdete" type="date">
                <constraints nullable="true" />
